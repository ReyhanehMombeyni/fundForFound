{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "Category"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "subcategories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::subcategory.subcategory",
      "mappedBy": "category"
    },
    "brand_orgs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::brand-org.brand-org",
      "mappedBy": "category"
    }
  }
}
